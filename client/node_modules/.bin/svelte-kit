#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/@sveltejs+kit@2.21.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.33.14_vite@5.4.19_@typ_6dee883c0bc2379f5338b35f4725d904/node_modules/@sveltejs/kit/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/@sveltejs+kit@2.21.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.33.14_vite@5.4.19_@typ_6dee883c0bc2379f5338b35f4725d904/node_modules/@sveltejs/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/@sveltejs+kit@2.21.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.33.14_vite@5.4.19_@typ_6dee883c0bc2379f5338b35f4725d904/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/@sveltejs+kit@2.21.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.33.14_vite@5.4.19_@typ_6dee883c0bc2379f5338b35f4725d904/node_modules/@sveltejs/kit/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/@sveltejs+kit@2.21.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.33.14_vite@5.4.19_@typ_6dee883c0bc2379f5338b35f4725d904/node_modules/@sveltejs/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/@sveltejs+kit@2.21.2_@sveltejs+vite-plugin-svelte@4.0.4_svelte@5.33.14_vite@5.4.19_@typ_6dee883c0bc2379f5338b35f4725d904/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
