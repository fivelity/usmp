#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/vite@5.4.19_@types+node@20._db87a25ca18c0d1a309c6225c3cc19e5/node_modules/vite/bin/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/vite@5.4.19_@types+node@20._db87a25ca18c0d1a309c6225c3cc19e5/node_modules/vite/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/vite@5.4.19_@types+node@20._db87a25ca18c0d1a309c6225c3cc19e5/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/vite@5.4.19_@types+node@20._db87a25ca18c0d1a309c6225c3cc19e5/node_modules/vite/bin/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/vite@5.4.19_@types+node@20._db87a25ca18c0d1a309c6225c3cc19e5/node_modules/vite/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/vite@5.4.19_@types+node@20._db87a25ca18c0d1a309c6225c3cc19e5/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
