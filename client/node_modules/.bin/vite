#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules/vite/bin/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules/vite/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules/vite/bin/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules/vite/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@5.4.19_@types+node@20.17.57_less@4.3.0_lightningcss@1.30.1_sass-embedded@1.89.1_sa_28e07df410769186d98cf41fd6546ba2/node_modules/vite/bin/vite.js" "$@"
fi
