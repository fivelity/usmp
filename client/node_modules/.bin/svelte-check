#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_199fc99148563cf2956dcd6a2bc8cd67/node_modules/svelte-check/bin/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_199fc99148563cf2956dcd6a2bc8cd67/node_modules/svelte-check/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_199fc99148563cf2956dcd6a2bc8cd67/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_199fc99148563cf2956dcd6a2bc8cd67/node_modules/svelte-check/bin/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_199fc99148563cf2956dcd6a2bc8cd67/node_modules/svelte-check/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_199fc99148563cf2956dcd6a2bc8cd67/node_modules:/proc/cygdrive/f/GitHub/ultimate-sensor-monitor-svelte.worktrees/markfive/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
