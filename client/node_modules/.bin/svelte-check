#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/svelte-check@3.8.6_less@4.3.0_postcss-load-config@4.0.2_postcss@8.5.4__postcss@8.5.4_sa_402c0f9c0ce8ba1157d2917f6a0d92f4/node_modules/svelte-check/bin/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/svelte-check@3.8.6_less@4.3.0_postcss-load-config@4.0.2_postcss@8.5.4__postcss@8.5.4_sa_402c0f9c0ce8ba1157d2917f6a0d92f4/node_modules/svelte-check/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/svelte-check@3.8.6_less@4.3.0_postcss-load-config@4.0.2_postcss@8.5.4__postcss@8.5.4_sa_402c0f9c0ce8ba1157d2917f6a0d92f4/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/svelte-check@3.8.6_less@4.3.0_postcss-load-config@4.0.2_postcss@8.5.4__postcss@8.5.4_sa_402c0f9c0ce8ba1157d2917f6a0d92f4/node_modules/svelte-check/bin/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/svelte-check@3.8.6_less@4.3.0_postcss-load-config@4.0.2_postcss@8.5.4__postcss@8.5.4_sa_402c0f9c0ce8ba1157d2917f6a0d92f4/node_modules/svelte-check/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/svelte-check@3.8.6_less@4.3.0_postcss-load-config@4.0.2_postcss@8.5.4__postcss@8.5.4_sa_402c0f9c0ce8ba1157d2917f6a0d92f4/node_modules:/home/jpfive/projects/ultimate-sensor-monitor-svelte5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
