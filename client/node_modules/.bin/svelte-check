#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules/svelte-check/bin/node_modules:/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules/svelte-check/node_modules:/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules:/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules/svelte-check/bin/node_modules:/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules/svelte-check/node_modules:/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules:/mnt/f/GitHub/ultimate-sensor-monitor-svelte/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules/svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/svelte-check@3.8.6_less@4.3_d7bbfea60b95ede2491dbe77dbc17d7e/node_modules/svelte-check/bin/svelte-check" "$@"
fi
